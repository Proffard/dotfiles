alias zshconfig='vim $dotfilespath/zshrc'
alias vimconfig='vim $dotfilespath/vimrc'
alias tmuxconfig='vim ~/.tmux.conf'
alias aliases='vim $dotfilespath/zsh_aliases'
alias ls='ls -a -F -G'
alias -g bake='bundle exec rake'
alias -g gake='setenv bundle exec rake'
alias -g honsole='heroku run console'
alias cdg='cd ~/workspace/GlobalATI'
alias logdev='tail -f log/development.log'
alias g='git'
alias gf='git fetch --all'
alias gs='git status'
alias ga='git add'
alias gd='git diff'
alias gc='git commit'
alias gg='git gro'
alias gc-='git commit --amend'
alias gac='git commit -a'
alias gac-='git commit -a --amend'
alias gpoh='git push origin HEAD'
alias h='heroku'
alias hr='heroku run'
alias hc='heroku config'
